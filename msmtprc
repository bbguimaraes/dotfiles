defaults
auth           on
tls            on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        ~/.msmtp.log

account        gmail
host           smtp.gmail.com
port           587
from           bbgstb@gmail.com
user           bbgstb
passwordeval   kwallet_read_password kdewallet mutt mutt msmtp

account default : gmail
